# .env

# Tells ADK to use the Vertex AI backend for API calls [cite: 78]
GOOGLE_GENAI_USE_VERTEXAI=TRUE

# Specifies your Google Cloud project [cite: 80]
GOOGLE_CLOUD_PROJECT=fsi-banking-agentspace

# Sets the location for the API endpoint, e.g., us-central1 [cite: 82]
GOOGLE_CLOUD_LOCATION=us-central1